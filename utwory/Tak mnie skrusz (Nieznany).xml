  <properties>
    <titles>
      <title>Tak mnie skrusz</title>
    </titles>
    <authors>
      <author>Nieznany</author>
    </authors>
    <themes>
      <theme>worship</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Tak mnie skrusz,<br/>tak mnie złam,<br/>tak mnie wypal, Panie,<br/>byś został tylko Ty,<br/>byś został tylko Ty,<br/>jedynie Ty.</lines>
    </verse>
  </lyrics>
