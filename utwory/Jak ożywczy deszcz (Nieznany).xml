<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.7" modifiedIn="OpenLP 3.1.7" modifiedDate="2026-02-06T21:07:38">
  <properties>
    <titles>
      <title>Jak ożywczy deszcz</title>
    </titles>
    <authors>
      <author>Nieznany</author>
    </authors>
    <themes>
      <theme>do Ducha Świętego</theme>
    </themes>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jak ożywczy deszcz<br/>Duchu Święty przyjdź<br/>Dotknij naszych serc<br/>Rozpal ognia żar</lines>
    </verse>
    <verse name="v2">
      <lines>Dziś przenikaj nas<br/>Tchnieniem mocy swej<br/>Bądź nam światłem dnia<br/>I nadzieją serc</lines>
    </verse>
  </lyrics>
</song>
