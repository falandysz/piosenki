<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.7" modifiedIn="OpenLP 3.1.7" modifiedDate="2026-01-05T14:45:15">
  <properties>
    <titles>
      <title>Wszystko mogę w Tym, który mnie umacnia</title>
    </titles>
    <authors>
      <author>Nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines><chord name="Em"/>Wszystko mogę w T<chord name="C"/>ym,<br/>który m<chord name="G"/>nie u<chord name="D"/>macnia.</lines>
    </verse>
    <verse name="v2">
      <lines><chord name="Em"/>Jez<chord name="C"/>us um<chord name="G"/>acnia mn<chord name="D"/>ie.</lines>
    </verse>
    <verse name="v3">
      <lines>R<chord name="Em"/>ozlewaj swoją ch<chord name="C"/>wałę,<br/>p<chord name="G"/>otęgę i m<chord name="D"/>oc<br/>n<chord name="Em"/>a miasto Twoje św<chord name="C"/>ięte,<br/>p<chord name="G"/>otęgę i m<chord name="D"/>oc.</lines>
    </verse>
  </lyrics>
</song>
