  <properties>
    <titles>
      <title>Wszystko mogę w Tym, który mnie umacnia</title>
    </titles>
    <authors>
      <author>Nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines><chord name="Dm"/>Wszystko mogę w T<chord name="B"/>ym,<br/>który m<chord name="F"/>nie u<chord name="C"/>macnia.</lines>
    </verse>
    <verse name="v2">
      <lines><chord name="Dm"/>Jez<chord name="B"/>us um<chord name="F"/>acnia mn<chord name="C"/>ie.</lines>
    </verse>
    <verse name="v3">
      <lines>R<chord name="Dm"/>ozlewaj swoją ch<chord name="B"/>wałę,<br/>p<chord name="F"/>otęgę i m<chord name="C"/>oc<br/>n<chord name="Dm"/>a miasto Twoje św<chord name="B"/>ięte,<br/>p<chord name="F"/>otęgę i m<chord name="C"/>oc.</lines>
    </verse>
  </lyrics>
</song>
