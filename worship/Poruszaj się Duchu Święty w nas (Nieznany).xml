<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.7" modifiedIn="OpenLP 3.1.7" modifiedDate="2026-02-05T17:03:09">
  <properties>
    <titles>
      <title>Poruszaj się Duchu Święty w nas</title>
    </titles>
    <authors>
      <author>Nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>P<chord name="Bm"/>oruszaj się D<chord name="G"/>uchu Święty w nas.<br/><br/>Roz<chord name="D"/>palaj serca <chord name="A"/>uwielbieniem Jezusa.</lines>
    </verse>
    <verse name="c1">
      <lines><chord name="Bm"/>Przyjdź <chord name="G"/>jak ogień!<br/><br/>Przyjdź <chord name="D"/>jak olej!<br/><br/>Przyjdź <chord name="A"/>jak wiatr!<br/><br/>Przyjdź <chord name="Hm"/>jak deszcz!</lines>
    </verse>
  </lyrics>
</song>
